{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  created() {\n    axios.get('https://jobs.tipiteq.com/dummy_data.json').then(response => {\n      this.tasks = Array.prototype.slice.call(Object.values(response.data));\n      this.tasks = this.tasks[0];\n      console.log(this.tasks);\n    }).catch(error => {\n      console.error('Error fetching tasks:', error);\n    });\n  }\n};","map":{"version":3,"names":["axios","name","data","tasks","created","get","then","response","Array","prototype","slice","call","Object","values","console","log","catch","error"],"sources":["/Users/gorkem/Desktop/vue-work/vue-project/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div v-for=\"task in tasks\" :key=\"task.id\" class=\"TaskBox\">\n      <div class=\"titles-task\">Task</div>\n      <hr>\n      <div class=\"titles-desc\">Açıklama:</div>\n      <div class=\"description\">{{ task.description }}</div>\n      <hr>\n      <div v-for=\"event in task.events\" :key=\"event\" class=\"events\">\n        <div class=\"titles-event\">Olaylar</div>\n        <div class=\"event\">\n          <div class=\"desc\">{{ event.description }}</div>\n          <div class=\"datetime\">{{ event.datetime }}</div>\n          <div class=\"type\">{{ event.type }}</div>\n          <div class=\"user\">\n            {{ event.user.name }}\n          </div>\n          <div v-for=\"tenant in event.user.tenants\" :key=\"tenant.id\" class=\"tenants\">\n            <div class=\"tenant\">{{ tenant.name }}</div>\n          </div>\n          <hr>\n        </div>\n        <hr>\n      </div>\n      Zaman :\n      <div class=\"occurrence\">{{ task.occurrence }}</div>\n      Öncelik : \n      <div class=\"priority\">{{ task.priority }}</div>\n      Sorumlular:\n      <div v-for=\"user in task.responsibleUsers\" :key=\"user.id\" class=\"responsibleUsers\">\n        <div class=\"info\">{{user.responsibleUser.name}}</div>\n      </div>\n      <div v-for=\"tenant in task.tenants\" :key=\"tenant.id\" class=\"tenantsArr\">\n        <div class=\"name\">{{ tenant.name }}</div>\n      </div>\n      <hr><hr><hr>\n    <hr><hr><hr><hr><hr>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      tasks: [],\n    }\n  },\n  created() {\n      axios.get('https://jobs.tipiteq.com/dummy_data.json')\n        .then(response => {\n          this.tasks = Array.prototype.slice.call(Object.values(response.data));\n          this.tasks = this.tasks[0]\n          console.log(this.tasks)\n        }).catch(error => {\n          console.error('Error fetching tasks:', error)\n        })\n  }\n}\n</script>\n\n<style>\n/* Stil tanımlamaları buraya */\n</style>\n"],"mappings":"AA0CA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNJ,KAAK,CAACK,GAAG,CAAC,0CAA0C,EACjDC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACJ,KAAI,GAAIK,KAAK,CAACC,SAAS,CAACC,KAAK,CAACC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACN,QAAQ,CAACL,IAAI,CAAC,CAAC;MACrE,IAAI,CAACC,KAAI,GAAI,IAAI,CAACA,KAAK,CAAC,CAAC;MACzBW,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,KAAK;IACxB,CAAC,CAAC,CAACa,KAAK,CAACC,KAAI,IAAK;MAChBH,OAAO,CAACG,KAAK,CAAC,uBAAuB,EAAEA,KAAK;IAC9C,CAAC;EACP;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}